<h1>Previously searched houses</h1>


<% @houses.each do |house| %>
<div class="house">
  <%# address =  %>
  <%= link_to house.street_address + ", " + house.zip.to_s, house_path(house.id) %>

  <p>
    searched on: <%= house.created_at.to_formatted_s(:long_ordinal).split(" ")[0..2].join(" ") %>
  </p>
</div>
<% end %>
<hr>
